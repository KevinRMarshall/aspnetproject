    <UserSecretsId>aspnet-OnlineGameStore-FC4C25B8-0E9D-4589-8692-F83F84D4A888</UserSecretsId>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
