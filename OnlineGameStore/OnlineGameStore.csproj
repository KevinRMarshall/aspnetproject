    <None Remove="Areas\Identity\images\select@2x.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Areas\Identity\images\arrow.png">
      <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
